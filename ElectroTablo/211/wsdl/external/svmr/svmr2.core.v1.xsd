<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://emias.mos.ru/svmr2/core/v1/"
           attributeFormDefault="unqualified" elementFormDefault="qualified"
           targetNamespace="http://emias.mos.ru/svmr2/core/v1/">
    <xs:include
            schemaLocation="emias.core.v1.xsd"/>
    <xs:include
            schemaLocation="emias.core.address.v1.xsd"/>
    <xs:include
            schemaLocation="svmr2.core.queryExpression.v1.xsd"/>
    <!-- Basic types -->
    <xs:complexType abstract="true" name="NullableType">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип, реализующий возможность изменения необязательных немножественных
                значений в NULL
            </xs:documentation>
        </xs:annotation>
        <xs:sequence maxOccurs="unbounded" minOccurs="0">
            <xs:element name="value">
                <xs:annotation>
                    <xs:documentation>Значение (нетипизированное)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="IDMultiNullable">
        <xs:annotation>
            <xs:documentation>Тип, реализующий возможность изменения множественных необязательных значений типа ID в
                NULL
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="NullableType">
                <xs:sequence maxOccurs="unbounded" minOccurs="0">
                    <xs:element name="value" type="ID">
                        <xs:annotation>
                            <xs:documentation>Значение типа ID</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IDNullable">
        <xs:annotation>
            <xs:documentation>Тип, реализующий возможность изменения необязательного значения типа ID в NULL
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="IDMultiNullable">
                <xs:sequence minOccurs="0">
                    <xs:element name="value" type="ID">
                        <xs:annotation>
                            <xs:documentation>Значение типа ID</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ShortMultiNullable">
        <xs:annotation>
            <xs:documentation>Тип, реализующий возможность изменения множественных необязательных значений типа short в
                NULL
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="NullableType">
                <xs:sequence maxOccurs="unbounded" minOccurs="0">
                    <xs:element name="value" type="xs:short">
                        <xs:annotation>
                            <xs:documentation>Значение типа short</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ShortNullable">
        <xs:annotation>
            <xs:documentation>Тип, реализующий возможность изменения необязательного значения типа short в NULL
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="ShortMultiNullable">
                <xs:sequence minOccurs="0">
                    <xs:element name="value" type="xs:short">
                        <xs:annotation>
                            <xs:documentation>Значение типа short</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="Base">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип для компонентов сущностей</xs:documentation>
        </xs:annotation>
        <xs:attribute name="id" type="ID" use="required">
            <xs:annotation>
                <xs:documentation>Идентификатор</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="DictionaryTypeBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип для справочника</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Base">
                <xs:sequence>
                    <xs:element name="code" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Код (бизнесовый идентификатор)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="EntityBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип, используемый для описания сущностей</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Base">
                <xs:attribute name="archived" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Признак "Архивная сущность"</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="EquipmentBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип с характеристиками Оборудования</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element minOccurs="0" name="model" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Модель оборудования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="inventoryNum" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Инвентарный номер</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="equipmentTypeNsiId" type="ID">
                <xs:annotation>
                    <xs:documentation>Вид оборудования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="mobilityCode" type="EquipmentMobilities">
                <xs:annotation>
                    <xs:documentation>Мобильность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="medicalOrganizationId"/>
            <xs:element minOccurs="0" name="addInfo" type="EquipmentAddInfo">
                <xs:annotation>
                    <xs:documentation>Дополнительные сведения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="capacity" type="xs:short">
                <xs:annotation>
                    <xs:documentation>Максимальное количество одновременно обслуживаемых на оборудовании пациентов в
                        соответствии с техническими характеристиками
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="personalizedService" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак индивидуального обслуживания. TRUE - индивидуальное обслуживание каждого
                        пациента. FALSE - одновременное обслуживание группы пациентов
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="treatments" type="TreatmentsEquipment">
                <xs:annotation>
                    <xs:documentation>Коллекция Медицинских манипуляций</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="archived" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Признак "Архивная сущность"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="LocationEquipmentBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип с характеристиками Оборудования в локации Медицинского
                учреждения
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element minOccurs="0" name="equipmentId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор оборудования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="equipmentProfileId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор профиля Оборудования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="departmentId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор Структурного подразделения Медицинской организации
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="locationId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор Локации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="capacityForRoom" type="xs:short">
                <xs:annotation>
                    <xs:documentation>Максимальное количество одновременно обслуживаемых на оборудовании пациентов с
                        учетом любых ограничивающих условий Локации (наличие коек, стульев, площади кабинета, количества
                        розеток и т.д.)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="treatments" type="TreatmentsLocationEquipment">
                <xs:annotation>
                    <xs:documentation>Коллекция Медицинских манипуляций</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="archived" type="xs:boolean" use="required">
            <xs:annotation>
                <xs:documentation>Признак "Архивная сущность"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="FacilityBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип для Учреждений</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Base"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="LocationBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип c характеристиками Локации</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element minOccurs="0" name="roomTitle" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер кабинета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Комментарий</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="stage" type="ShortNullable">
                <xs:annotation>
                    <xs:documentation>Этаж</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="medicalFacilityId"/>
        </xs:sequence>
        <xs:attribute name="archived" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Признак "Архивная сущность"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="MedicalRoomBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип c характеристиками Медицинского блока</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element minOccurs="0" name="roomTitle" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер кабинета</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="stage" type="ShortNullable">
                <xs:annotation>
                    <xs:documentation>Этаж</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="educationFacilityId"/>
            <xs:element minOccurs="0" ref="medicalOrganizationId"/>
            <xs:element minOccurs="0" name="period" type="RequredStartPeriod"/>
        </xs:sequence>
        <xs:attribute name="archived" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Признак "Архивная сущность"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="MedicalOrganizationBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип c характеристиками Медицинской организации</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Полное наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="shortName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Краткое наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ogrn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="okopf" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код ОКОПФ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="inn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="kpp" type="xs:string">
                <xs:annotation>
                    <xs:documentation>КПП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="okpo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ОКПО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="regDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации в ЕГРЮЛ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="period" type="RequredStartPeriod">
                <xs:annotation>
                    <xs:documentation>Период работы медицинской организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="departmentAffilation" type="IDNullable">
                <xs:annotation>
                    <xs:documentation>Ведомственная принадлежность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="medicalActivityNsiId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор Вида медицинской деятельности организации из справочника
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="specializationDescription" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Описание специализации медицинской организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="certForPeopleWithDisabilities" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак того, что МО осуществляет выдачу Медицинский заключений для ЦПМПК
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="historyInfo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Описание истории создания, реорганизации и закрытия МО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="oid" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Объектный идентификатор объекта МО по ЕГИСЗ, присваивается в ЕГИСЗ по отдельной
                        заявке/письму
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="organizationAddress"
                        type="MedicalOrganizationAddress">
                <xs:annotation>
                    <xs:documentation>Адреса организации</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="archived" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>Признак "Архивная сущность"</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType abstract="true" name="MedicalFacilityBase">
        <xs:annotation>
            <xs:documentation>Базовый абстрактный тип c характеристиками Медицинского учреждения</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="medicalOrganizationId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор МО, к которому относится МУ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="currentName" type="NameFacility">
                <xs:annotation>
                    <xs:documentation>Текущее наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="independence" type="IDNullable">
                <xs:annotation>
                    <xs:documentation>Обособленность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="medicalFacilityType" type="IDNullable">
                <xs:annotation>
                    <xs:documentation>Тип МУ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="period" type="RequredStartPeriod">
                <xs:annotation>
                    <xs:documentation>Период функционирования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="name" type="NameFacility">
                <xs:annotation>
                    <xs:documentation>Прежние наименования</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="clo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код ЦЛО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="mgfoms" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код МГФОМС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="addressObject" type="AddressObjectExtension">
                <xs:annotation>
                    <xs:documentation>Адресный объект</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="emergencyStation" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак наличия медицинской станции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="emergencyBrigades" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество бригад, заполняется только при значении поля "Наличие станции" true
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="phoneNumber" type="FacilityPhoneUpdatable">
                <xs:annotation>
                    <xs:documentation>Номер телефона, заполняется только при заполненном поле "Тип номера телефона"
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="isHead" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>Является Головным</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!-- Extension types -->
    <xs:complexType name="MedicalOrganizationCreatable">
        <xs:annotation>
            <xs:documentation>Сведения для создания новой Медицинской организации</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Полное наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shortName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Краткое наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ogrn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="okopf" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Код ОКОПФ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="kpp" type="xs:string">
                <xs:annotation>
                    <xs:documentation>КПП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="okpo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ОКПО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="regDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации в ЕГРЮЛ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="period" type="RequredStartPeriod">
                <xs:annotation>
                    <xs:documentation>Период работы медицинской организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="departmentAffilation" type="IDNullable">
                <xs:annotation>
                    <xs:documentation>Ведомственная принадлежность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="medicalActivityNsiId" type="ID">
                <xs:annotation>
                    <xs:documentation>Идентификатор Вида медицинской деятельности организации из справочника
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="specializationDescription" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Описание специализации медицинской организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="certForPeopleWithDisabilities" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак того, что МО осуществляет выдачу Медицинский заключений для ЦПМПК
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="historyInfo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Описание истории создания, реорганизации и закрытия МО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="oid" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Объектный идентификатор объекта МО по ЕГИСЗ, присваивается в ЕГИСЗ по отдельной
                        заявке/письму
                    </xs:documentation>
                </xs:annotation>
